<templ6> a toct:Template
    ; toct:supportsLanguage <translZu>
    ; toct:hasFirstPart <op>
    ; toct:hasLastPart <c> .

<translZu> a mola:Dialect
    ; mola:isFamily <isiZulu> .

<op> a toct:Slot
    ; toct:hasLabel "OP"^^xsd:string
    ; toct:hasNextPart <c> .

<c> a toct:Slot
    ; toct:hasLabel "C"^^xsd:string .
