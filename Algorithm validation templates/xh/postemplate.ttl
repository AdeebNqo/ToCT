@base <http://people.cs.uct.ac.za/~zmahlaza/templates/test/> .
@prefix  toct: <https://people.cs.uct.ac.za/~zmahlaza/ontologies/ToCT#> .
@prefix  mola: <https://ontology.londisizwe.org/mola#> .
@prefix  owl: <http://www.w3.org/2002/07/owl#> .
@prefix  rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix  xml: <http://www.w3.org/XML/1998/namespace> .
@prefix  xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix  rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix  co: <https://people.cs.uct.ac.za/~zmahlaza/ontologies/CO#> .
@prefix xhtemp: <https://people.cs.uct.ac.za/~zmahlaza/ontologies/templates/XhTemplateAnnotationOntology#> .

<possessiveNounTemp> a toct:Template
	; toct:supportsLanguage <cpt_xhosa>
	; co:firstItem <noun1Slot>
	; co:lastItem <ofmen>
	; co:item  <noun1Slot>, <ofmen> .

<cpt_xhosa> a mola:Dialect
	; mola:isFamily <isiXhosa> .

<noun1Slot> a toct:Slot
	; co:index "1"^^xsd:positiveInteger
	; toct:labeledWith <posNounType>
	; toct:hasLabel "noun1Slot"^^xsd:string
	; co:nextItem <ofmen> .

<ofmen> a toct:PolymorphicWord
	; co:index "2"^^xsd:positiveInteger
	; toct:reliesOn <noun1Slot>
	; co:firstItem <of>
	; co:lastItem <fv>
	; co:item <of>, <maAffix>, <men>, <fv> .

<of> a toct:Concord
	; co:index "1"^^xsd:positiveInteger
	; xhtemp:hasConcordType <possConType>
	; toct:hasLabel "POSSE"^^xsd:string
	; toct:dependsOn <noun1Slot>
	; co:nextItem <maAffix> .

<maAffix> a toct:UnimorphicAffix
    ; co:index "2"^^xsd:positiveInteger
    ; toct:hasValue "ma"^^xsd:string
    ; co:nextItem <men> .

<men> a toct:Root
	; co:index "3"^^xsd:positiveInteger
	; toct:hasValue "doda"^^xsd:string
	; co:nextItem <fv> .

<fv> a toct:UnimorphicAffix
    ; co:index "4"^^xsd:positiveInteger
    ; toct:hasValue "a"^^xsd:string .

<possConType> a xhtemp:PossessiveConcord .

<posNounType> a xhtemp:Noun .
